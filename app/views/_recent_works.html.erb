<div class="section section-breadcrumbs">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h1><%= element.title %></h1>
      </div>
    </div>
  </div>
</div>

<div class="section">
  <div class="container">
    <div class="row">
      <!-- Product Image & Available Colors -->
      <div class="col-sm-6">
        <div class="product-image-large">
          <%= image_tag element.image %>

        </div>
        <div class="colors">
          <span class="color-white"></span>
          <span class="color-black"></span>
          <span class="color-blue"></span>
          <span class="color-orange"></span>
          <span class="color-green"></span>
        </div>
      </div>
      <!-- End Product Image & Available Colors -->
      <!-- Product Summary & Options -->
      <div class="col-sm-6 product-details">



        <p><strong>Tecnica: </strong><%= element.technic %></p>
        <p><strong>Dimension: </strong><%= element.dimentions %></p>
        <p><strong>Anno: </strong><%=element.year %></p>










        <!--<p><strong>Date: </strong>August 21, 2015</p>
        <p><strong>Tags: </strong>Photography, Art, Graphics</p>-->
      </div>
      <!-- End Product Summary & Options -->

    </div>
  </div>
</div>
